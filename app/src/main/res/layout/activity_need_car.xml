<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:src="@+id/activity_need_car"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.anew.toura.org.example.anew.toura.NeedCarActivity">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/threecars"
        android:layout_marginTop="91dp"
        android:id="@+id/imageView2"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="11dp" />

    <TextView
        android:text="+8801710838622"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:textColor="?android:attr/textColorPrimaryInverseNoDisable"
        android:textSize="20sp"
        android:textAlignment="center"
        android:background="@android:color/black"
        android:padding="20dp"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/mail"
        android:id="@+id/SmsBtn"
        android:layout_marginEnd="85dp"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/textView2"
        android:layout_alignEnd="@+id/imageView2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/callbuttun"
        android:id="@+id/callBtn"
        android:layout_alignTop="@+id/SmsBtn"
        android:layout_alignEnd="@+id/SmsBtn"
        android:layout_marginEnd="88dp" />

</RelativeLayout>
